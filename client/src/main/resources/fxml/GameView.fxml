<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="gameAnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="de.uol.swp.client.game.GamePresenter">
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="gameAnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uol.swp.client.game.GamePresenter">
    <children>
        <TextArea fx:id="gameChatArea" editable="false" layoutX="370.0" layoutY="231.0" prefHeight="114.0" prefWidth="224.0" GridPane.rowIndex="2" />

        <ListView fx:id="gameUsersView" layoutX="370.0" layoutY="30.0" prefHeight="90.0" prefWidth="224.0" />
        <ToolBar layoutY="-5.0" prefHeight="35.0" prefWidth="600.0">
            <items>
                <Button mnemonicParsing="false" onAction="#onRollDice" text="Roll Dice" />
                <Text fx:id="DiceOutput" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                <Separator orientation="VERTICAL" />
                <Button mnemonicParsing="false" onAction="#onTrade" text="Trade">
                </Button>
                <MenuButton layoutX="103.0" layoutY="11.0" mnemonicParsing="false" text="Build">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onBuildStreet" text="Street" />
                        <MenuItem mnemonicParsing="false" onAction="#onBuildSettlement" text="Settlement" />
                        <MenuItem mnemonicParsing="false" onAction="#onBuildTown" text="Town" />
                    </items>
                </MenuButton>
                <Button layoutX="153.0" layoutY="11.0" mnemonicParsing="false" onAction="#onBuyDevelopmentCard" text="Buy DevelopmentCard" />
                <Button fx:id="EndTurnButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onEndTurn" text="End Turn" />
                <Region prefHeight="25.0" prefWidth="73.0" />
                <Button mnemonicParsing="false" onAction="#onLeaveGame" text="Leave Game" />
            </items>
        </ToolBar>
        <Canvas fx:id="canvas" height="370.0" layoutX="0.0" layoutY="30.0" width="370" />
        <Button layoutX="534.0" layoutY="345.0" mnemonicParsing="false" onAction="#onSendMessage" prefHeight="25.0" prefWidth="59.0" text="Send" />
        <TextField fx:id="gameChatInput" layoutX="370.0" layoutY="345.0" onAction="#onSendMessage" prefHeight="25.0" prefWidth="161.0" promptText="Send Message.." />
      <Canvas id="inventory" fx:id="inventory" height="104.0" layoutX="370.0" layoutY="120.0" width="224.0" />
      <Pane fx:id="picturePlayerView1" layoutX="600.0" layoutY="30.0" prefHeight="90.0" prefWidth="106.0" />
      <Pane fx:id="picturePlayerView2" layoutX="600.0" layoutY="163.0" prefHeight="104.0" prefWidth="106.0" />
      <Pane fx:id="picturePlayerView3" layoutX="722.0" layoutY="30.0" prefHeight="90.0" prefWidth="92.0" />
      <Pane fx:id="picturePlayerView4" layoutX="706.0" layoutY="148.0" prefHeight="104.0" prefWidth="106.0" />
    </children>
</AnchorPane>
