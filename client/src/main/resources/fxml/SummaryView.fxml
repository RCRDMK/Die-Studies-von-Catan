<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="summary" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400"
            prefWidth="600.0"
            stylesheets="@../css/swp.css" fx:controller="de.uol.swp.client.game.SummaryPresenter">
    <children>
        <Button layoutX="245.0" layoutY="360.0" onAction="#onBackToMainMenu" text="Back to Main Menu"/>
        <Label fx:id="winnerLabel" style="-fx-font-weight: BOLD" alignment="CENTER" layoutX="205.0" layoutY="120.0"
               prefHeight="17.0" prefWidth="200.0" textAlignment="CENTER"/>
        <ImageView fx:id="winnerImage" fitHeight="110.0" fitWidth="100.0" layoutX="195.0" layoutY="10.0"
                   pickOnBounds="true" preserveRatio="true">
        </ImageView>
        <ImageView fx:id="profileImage" fitHeight="100.0" fitWidth="100.0" layoutX="314.0" layoutY="10.0"
                   pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="314.0">
        </ImageView>
        <TableView fx:id="statsTable" layoutX="100" layoutY="150.0" prefHeight="200.0" prefWidth="400">
            <columns>

            </columns>
        </TableView>
    </children>
</AnchorPane>
