<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="407.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uol.swp.client.register.RegistrationPresenter">
    <Label layoutX="44.0" layoutY="17.0" text="Login" />
    <TextField id="loginField" fx:id="loginField" layoutX="142.0" layoutY="12.0" promptText="Enter Login">
        <tooltip>
            <Tooltip text="Enter your login here" />
        </tooltip>
    </TextField>
    <PasswordField id="passwordField" fx:id="passwordField1" layoutX="142.0" layoutY="48.0" promptText="Enter Password" />
    <PasswordField id="passwordField" fx:id="passwordField2" layoutX="142.0" layoutY="85.0" promptText="Repeat Password" />
    <Label layoutX="44.0" layoutY="53.0" text="Password" />
    <Label layoutX="44.0" layoutY="90.0" text="Password" />
    <TextField id="emailField" fx:id="emailField1" layoutX="142.0" layoutY="123.0" promptText="Enter E-Mail" />
    <TextField id="emailField" fx:id="emailField2" layoutX="142.0" layoutY="163.0" promptText="Repeat E-Mail" />
    <Label layoutX="44.0" layoutY="127.0" text="E-Mail" />
    <Label layoutX="44.0" layoutY="167.0" text="E-Mail" />
    <Button id="registerButton" defaultButton="true" layoutX="112.0" layoutY="213.0" mnemonicParsing="false" onAction="#onRegisterButtonPressed" text="Register" />
    <Button id="cancelButton" layoutX="204.0" layoutY="213.0" mnemonicParsing="false" onAction="#onCancelButtonPressed" text="Cancel" />
</AnchorPane>
